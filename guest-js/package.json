{
  "name": "@rgbitsoft/tauri-plugin-unified-log",
  "version": "1.0.1",
  "description": "Unified logging bridge for Tauri. Consolidates Frontend (JS) and Backend (Rust) logs into a single terminal with a professional layout.",
  "author": "senspond20 <senspond@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/senspond20/tauri-plugin-unified-log.git"
  },
  "keywords": [
    "tauri",
    "plugin",
    "logging",
    "unified-log",
    "rgbitsoft"
  ],
  "type": "module",
